{
  "version": 1,
  "name": "great78.archive.org",
  "home": {
    "get": "http://great78.archive.org"
  },
  "news": {
    "get": "https://blog.archive.org/",
    "list": ".post",
    "title": ".entry-title",
    "details": ".entry-title a:regex(.*href=\"([^\"]*)\".*)",
    "date": ".entry-date:regex((.*))",
    "next": ".nav-previous a:regex(.*href=\"([^\"]*)\".*)"
  },
  "crawl": {
    "get": "crawls",
    "list": "div.item-ia:not(.mobile-header)",
    "title": ".ttl",
    "details": ".item-ttl > a:regex(.*href=\"([^\"]*)\".*)",
    "image": ".item-img:regex([^>]*source=\"([^\"]*)\".*)",
	"date": ".pubdate > nobr:nth-child(1)",
    "next": ".more_search:regex(.*href=\"([^\"]*)\".*)"
  },
  "crawls": {
    "New": "https://archive.org/details/78rpm?&sort=-publicdate&page=1"
  },
  "top": {
    "get": "tops",
    "grid": "crawl",
    "torrent_filter": "* - *.mp3",
    "update": "details"
  },
  "tops": {
    "Recents": "trim(substr(date,8,4))||substr(date,0,4)||substr('0'||trim(substr(date,5,2),','), -2, 2) desc"
  },
  "details": {
    "torrent": "a.download-pill[href$=torrent]:regex(.*href=\"([^\"]*)\".*)"
  },
  "search": {
    "get": "trim(substr(date,8,4))||substr(date,0,4)||substr('0'||trim(substr(date,5,2),','), -2, 2) desc",
    "grid": "crawl",
    "torrent_filter": "* - *.mp3",
    "update": "details"
  }
}
