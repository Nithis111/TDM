  },
  "details": {
    "downloads": "div.col-sm-4 > div:nth-child(3) > div:nth-child(6)",
    "torrent": "a.download-pill[href$=torrent]:regex(.*href=\"([^\"]*)\".*)"
  },
  "search": {
    "get": "https://archive.org/details/78rpm?sort=-publicdate&and[]=",
    "grid": "div.item-ia:not(.mobile-header)",
