    "image": ".item-img:regex([^>]*source=\"([^\"]*)\".*)",
    "date": ".pubdate > nobr:nth-child(1)",
    "downloads": ".stat nobr",
    "next": ".more_search:regex(.*href=\"([^\"]*)\".*)",
    "update": "details",
    "torrent_filter": "*.mp3"
  },
  "tops": {
    "Views": "https://archive.org/details/78rpm?and%5B%5D=mediatype%3A%22audio%22&sort=-downloads",
